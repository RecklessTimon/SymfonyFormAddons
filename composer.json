{
    "name": "recklesstimon/symfony-form-addons",
    "type": "library",
    "description": "Symfony form addons",
    "keywords": ["Symfony form", "Serializer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Butov"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/form": "^5.0",
        "symfony/serializer": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "RT\\SymfonyFormAddons\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RT\\SymfonyFormAddons\\Tests\\": "/Tests/"
        }
    }
}
